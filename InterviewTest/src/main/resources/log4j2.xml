<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>



	<Appenders>
		<Console name="console">
			<PatternLayout>
				<Pattern>%d{HH:mm:ss,SSS} [%t] [%p] %c{2} - %m%n</Pattern>
			</PatternLayout>
		</Console>


	</Appenders>


	<Loggers>

		<Root level="info">
			<AppenderRef ref="console" />
		</Root>
	</Loggers>

</Configuration>
